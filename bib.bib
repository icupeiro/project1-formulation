@article{GP_kiuhn,
	title = "Gaussian Process Model for Control of an Existing Building",
	journal = "Energy Procedia",
	volume = "78",
	pages = "2136 - 2141",
	year = "2015",
	note = "6th International Building Physics Conference, IBPC 2015",
	issn = "1876-6102",
	doi = "https://doi.org/10.1016/j.egypro.2015.11.295",
	url = "http://www.sciencedirect.com/science/article/pii/S1876610215020275",
	author = "Ki-Uhn Ahn and Deuk-Woo Kim and Young-Jin Kim and Cheol-Soo Park and In-Han Kim",
	keywords = "Gaussian Process model, Data-driven model, RANSAC, data filtering, Building Energy Management System (BEMS)",
	abstract = "This study reports the development of a Gaussian Process (GP) model based on BEMS data. The GP model is a data driven model and requires a few dominant inputs. It provides a quick prediction with a far less computation than the whole building simulation tools (e.g. EnergyPlus). The GP model developed in this study is capable of predicting the behavior of a building system (fan energy consumption). This paper reports how the authors developed the GP model. In particular, this paper addresses how to deal with outliers existing in BEMS data set. In this study, RANdom Sample Consensus (RANSAC) was selected for detecting the outliers from the data set. The RANSAC method can be beneficially applied to improve the accuracy of the GP model."
}


