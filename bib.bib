  annote ={Pointed out by reviewer #4, Observability paper, IEEE, 1999:
        For general questions of NP-hardness for controllability of
        nonlinear systems, one should cite the first paper [Son88]
        Of course, this paper was not for PL systems as such, but for PL systems the
        reference [Son81], together with the results of [Son85]
        indicated clearly (implicitly, but way before [30]) that questions such as
        controllability should be NP-hard for such systems.  (Because they are purely
        existential formulas in real theory with addition, and those are shown to be
        generally equivalent to NP.  Of course, there is "bookeeping" needed in order
        to write as a PL system.)},
}


@article{JGD99,
  author =  {P. Julian and J. Guivant and A. Desages},
  title =   { A parametrization of piecewise linear
  Lyapunov functions via linear programming},
  journal = {IJC},
  year =    {1999},
  month =   May,
  volume =  {72},
  number={7-8},
  pages =   {702--715},
  abstract={We present a parametrization of piecewise linear (PWL) Lyapunov
functions. To this end, we consider the class of all continuous PWL
functions defined over a simplified partition. We take advantage of a
recently developed high level canonical PWL (HL CPWL) representation,
which expresses the PWL function in a compact and closed form. Once the
parametrization problem is properly stated, we focus on its application
to the stability analysis of dynamic systems. We consider uncertain
nonlinear systems and extend the sector condition obtained by Ohta et al.
(1993). In addition, we propose a method of selecting an optimal
candidate. One of the main advantages of this approach is that the
parametrization and choice of the Lyapunov candidate, as well as the
stability analysis, result in linear programming problems.},
}

@article{OIGH93,
  author =  {Y. Ohta and H. Imanishi and L. Gong and H. Haneda},
  title =   { Computer generated Lyapunov functions for a class of nonlinear systems},
  journal = {TCS},
  year =    {1993},
  month =   May,
  volume =  {40},
  number={5},
  pages =   {343--354},
  abstract={The problem of constructing Lyapunov functions for a class of
  nonlinear dynamical systems is considered. The problem is reduced to
  the construction of a polytope satisfying some conditions. A
  generalization of the concept of sector condition that it makes
  possible to evaluate a given nonlinear function by using a set of
  piecewise-linear functions is proposed. This improvement greatly
  reduces the conservatism in the stability analysis of nonlinear
  systems. Two algorithms for constructing such polytopes are proposed,
  and two examples are shown to demonstrate the usefulness of the
  results.
},
  }

